{
  "name": "@moonrepo/build-proto-plugin",
  "version": "0.1.2",
  "description": "A GitHub action to build, optimize, and prepare proto WASM plugins for release.",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build ./index.ts",
    "check": "pnpm run lint && pnpm run test && pnpm run typecheck",
    "deps": "pnpm update --latest --interactive",
    "lint": "eslint --ext .ts,.js --fix .",
    "test": "echo 'Not yet'",
    "test:input": "ts-node ./index.ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moonrepo/build-proto-plugin"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1",
    "@ltd/j-toml": "^1.38.0"
  },
  "devDependencies": {
    "@types/node": "^20.7.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.50.0",
    "eslint-config-moon": "^2.0.10",
    "prettier": "^3.0.3",
    "prettier-config-moon": "^1.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-moon": "^1.3.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
